public void redirectToPilotUrl(HttpServletRequest req, HttpServletResponse resp,
                                   String servletPath, String pathInfo, String rfcVersion) throws IOException {

        StringJoiner sPilotUrl = new StringJoiner("");
        String sPilotVip;

        if (req.getRequestURL().toString().contains("localhost")) {
            sPilotVip = "http://" + req.getServerName()
                    + (req.getRequestURL().toString().contains(".oneadp.com") ? "" : (":" + req.getServerPort()));
        } else {
            sPilotVip = "https://" + req.getServerName()
                    + (req.getRequestURL().toString().contains(".oneadp.com") ? "" : (":" + req.getServerPort()));
        }

        String pilotUrl = sPilotUrl.add(sPilotVip)
                .add(req.getContextPath())
                .add(rfcVersion)
                .add(servletPath)
                .add(pathInfo)
                .toString();

        log.info("**** REDIRECT TO PILOT URL FROM LSF Admin ****");
        log.info("sPilotUrl: {}", sPilotUrl.toString());
        log.info("getRequestURL(): {}", req.getRequestURL());
        log.info("req.getServerPort(): {}", req.getServerPort());
        log.info("req.getServerName(): {}", req.getServerName());
        log.info("sPathInfo: {}", pathInfo);
        log.info("**** REDIRECT TO PILOT URL TRACE ENDS ****");

        String sRemoteUser = req.getRemoteUser() != null ? req.getRemoteUser() : "UNKNOWN";
        log.info("Redirecting user {} to {}", sRemoteUser, pilotUrl);

        resp.sendRedirect(pilotUrl);
    }
